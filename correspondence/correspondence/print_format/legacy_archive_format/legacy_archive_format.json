 "html": "<div class=\"print-format-gutter\">\n    <div class=\"print-heading\">\n        <h2>Legacy Archive Record</h2>\n        <h4>{{ doc.name }}</h4>\n    </div>\n\n    <div class=\"row\">\n        <div class=\"col-xs-6\">\n            <div class=\"row\">\n                <div class=\"col-xs-4 text-right\"><label>Subject</label></div>\n                <div class=\"col-xs-8\">{{ doc.subject }}</div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-4 text-right\"><label>Date</label></div>\n                <div class=\"col-xs-8\">{{ frappe.utils.formatdate(doc.date) }}</div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-4 text-right\"><label>Type</label></div>\n                <div class=\"col-xs-8\">{{ doc.document_type }}</div>\n            </div>\n        </div>\n        <div class=\"col-xs-6\">\n            <div class=\"row\">\n                <div class=\"col-xs-4 text-right\"><label>Legacy Ref</label></div>\n                <div class=\"col-xs-8\">{{ doc.legacy_reference_number or '-' }}</div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-4 text-right\"><label>Location</label></div>\n                <div class=\"col-xs-8\">{{ doc.physical_location or '-' }}</div>\n            </div>\n        </div>\n    </div>\n\n    <hr>\n\n    <div class=\"row\">\n        <div class=\"col-xs-6\">\n            <div class=\"row\">\n                <div class=\"col-xs-4 text-right\"><label>Source</label></div>\n                <div class=\"col-xs-8\">{{ doc.source or '-' }}</div>\n            </div>\n        </div>\n        <div class=\"col-xs-6\">\n            <div class=\"row\">\n                <div class=\"col-xs-4 text-right\"><label>Destination</label></div>\n                <div class=\"col-xs-8\">{{ doc.destination or '-' }}</div>\n            </div>\n        </div>\n    </div>\n\n    <hr>\n\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <label>Summary</label>\n            <p>{{ doc.summary or 'No summary available.' }}</p>\n        </div>\n    </div>\n\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <label>Keywords</label>\n            <p>{{ doc.keywords or '-' }}</p>\n        </div>\n    </div>\n\n    {% if doc.attachments %}\n    <hr>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h4>Attachments</h4>\n            <table class=\"table table-bordered table-condensed\">\n                <thead>\n                    <tr>\n                        <th>File Name</th>\n                        <th>Type</th>\n                        <th>Size</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {% for row in doc.attachments %}\n                    <tr>\n                        <td>{{ row.file_name }}</td>\n                        <td>{{ row.file_type }}</td>\n                        <td>{{ row.file_size }} bytes</td>\n                    </tr>\n                    {% endfor %}\n                </tbody>\n            </table>\n        </div>\n    </div>\n    {% endif %}\n</div>",
